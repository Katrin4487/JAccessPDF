{
  "page-master-styles": [
    {
      "name": "main-content",
      "page-height": "29.7cm",
      "page-width": "21cm",
      "margin": "2cm",
      "column-count": "2",
      "column-gap": "1cm",
      "header-extent": "5cm"
    }
  ],
  "text-styles": [
    {
      "name": "headline",
      "font-size": "13pt",
      "font-family-name": "Arial",
      "font-weight": "700",
      "font-style": "normal"
    },
    {
      "name": "headline2",
      "font-size": "16pt",
      "font-family-name": "Open Sans",
      "font-weight": "400",
      "font-style": "normal"
    },
    {
      "name": "normal-text",
      "font-size": "11pt",
      "font-family-name": "Arial",
      "font-weight": "400",
      "font-style": "normal"
    },
    {
      "name": "emphasis",
      "font-size": "11pt",
      "font-family-name": "Open Sans",
      "font-weight": "400",
      "font-style": "italic"
    },
    {
      "name": "bold",
      "font-size": "11pt",
      "font-family-name": "Open Sans",
      "font-weight": "700",
      "font-style": "normal"
    }
  ],
  "element-styles": [
    {
      "name": "h1-style-all",
      "target-element": "headline",
      "properties": {
        "linefeed-treatment": "preserve",
        "text-style-name": "headline",
        "span": "all",
        "line-height": "1",
        "space-after": "0pt"
      }
    },
    {
      "name": "h1-style",
      "target-element": "headline",
      "properties": {
        "text-style-name": "headline",
        "space-before":"0cm",
        "line-height": "1"
      }
    },
    {
      "name": "gray-section",
      "target-element": "section",
      "properties": {
        "background-color": "gray",
        "padding": "0cm",
        "space-before": "0pt"
      }
    },
    {
      "name": "standard-paragraph",
      "target-element": "paragraph",
      "properties": {
        "text-style-name": "normal-text",
        "text-align": "justify"
      }
    }
  ]
}