{
  "page-master-styles": [
    {
      "name": "main-content",
      "page-height": "29.7cm",
      "page-width": "21cm",
      "margin-right": "1cm",
      "margin-top": "1cm",
      "margin-left": "2cm",
      "margin-bottom": "2cm",
      "column-count": "2",
      "column-gap": "0.2cm",
      "header-extent": "2cm",
      "footer-extent": "9cm"
    }
  ],
  "text-styles": [
    {
      "name": "headline",
      "font-size": "13pt",
      "font-family-name": "Arial",
      "font-weight": "700",
      "font-style": "normal"
    },
    {
      "name": "normal-text",
      "font-size": "8.5pt",
      "font-family-name": "Arial",
      "font-weight": "400",
      "font-style": "normal"
    },
    {
      "name": "big-text",
      "font-size": "13pt",
      "font-family-name": "Arial",
      "font-weight": "400",
      "font-style": "normal"
    },
    {
      "name": "bold-text",
      "font-size": "8.5pt",
      "font-family-name": "Open Sans",
      "font-weight": "700",
      "font-style": "normal"
    }
  ],
  "element-styles": [
    {
      "name": "h1-style-all",
      "target-element": "headline",
      "properties": {
        "linefeed-treatment": "preserve",
        "text-style-name": "headline",
        "span": "all",
        "line-height": "1.5",
        "space-after": "0.5cm"
      }
    },
    {
      "name": "h1-style",
      "target-element": "headline",
      "properties": {
        "text-style-name": "headline",
        "space-before":"0cm",
        "start-indent": "0cm",
        "line-height": "1.5",
        "space-after": "7pt",
        "linefeed-treatment": "preserve"
      }
    },
    {
      "name": "gray-section",
      "target-element": "section",
      "properties": {
        "background-color": "#EAEAE9",
        "padding": "0.5cm",
        "space-before": "0.2cm",
        "space-after": "0.2cm",
        "start-indent": "0.5cm",
        "end-indent": "0.5cm"
      }
    },
    {
      "name": "gray-section-last",
      "target-element": "section",
      "properties": {
        "background-color": "#EAEAE9",
        "padding": "0.5cm",
        "space-before": "0.2cm",
        "space-after": "0.2cm",
        "start-indent": "0.5cm",
        "end-indent": "0.5cm",
        "padding-bottom": "0.6cm"
      }
    },
    {
      "name": "standard-paragraph",
      "target-element": "paragraph",
      "properties": {
        "text-style-name": "normal-text",
        "space-after": "0.2cm"
      }
    },
    {
      "name": "spaces-paragraph",
      "target-element": "paragraph",
      "properties": {
        "text-style-name": "normal-text",
        "text-align": "justify",
        "space-before": "1cm"
      }
    },
    {
      "name": "bold-paragraph",
      "target-element": "paragraph",
      "properties": {
        "text-style-name": "bold-text",
        "text-align": "justify",
        "space-before": "0.3cm",
        "space-after": "0.2cm"
      }
    },
    {
      "name": "logo-block",
      "target-element": "block-image",
      "properties": {
        "alignment": "right",
        "block-width": "100%"
      }
    },
    {
      "name": "center-block",
      "target-element": "block-image",
      "properties": {
        "alignment": "center"
      }
    },
    {
      "name": "center-block-width",
      "target-element": "block-image",
      "properties": {
        "alignment": "center",
        "content-width": "22cm"
      }
    },
    {
      "name" : "step-list",
      "target-element": "list",
      "properties": {
        "provisional-distance-between-starts" :  "0.5cm",
        "list-style-position" : "outside",
        "text-style-name": "bold-text",
        "space-before": "0.2cm"
      }
    },
    {
      "name": "inline-bold",
      "target-element": "text-run",
      "properties": {
        "text-style-name": "bold-text",
        "text-color": "black"
      }
    },
    {
      "name": "big-paragraph-style-all",
      "target-element": "paragraph",
      "properties": {
        "linefeed-treatment": "preserve",
        "text-style-name": "big-text",
        "span": "all",
        "space-before": "0.1cm",
        "space-after": "0pt",
        "text-align": "center"
      }
    }
  ]
}